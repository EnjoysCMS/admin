<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>{{ breadcrumbs.lastBreadcrumb }}</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    {% for breadcrumb in breadcrumbs %}
                    <li class="breadcrumb-item"><a href="{{ breadcrumb.url }}">{{ breadcrumb.title }}</a></li>
                    {% endfor %}
                    <li class="breadcrumb-item active">{{ breadcrumbs.lastBreadcrumb }}</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
