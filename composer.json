{
    "name": "enjoyscms/admin",
    "description": "Module Admin for EnjoysCMS",
    "type": "enjoyscms-module",
    "require": {
        "php": ">=8.0",
        "enjoyscms/core": "^4.0",
        "enjoys/forms": "5.x-dev",
        "enjoys/bootstrap4-renderer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Module\\Admin\\": "src/"
        }
    },
    "extra": {
        "module-name": "Admin panel"
    },
    "scripts": {
        "post-install-cmd": "App\\Module\\Admin\\Composer::assetsInstall",
        "post-update-cmd": "App\\Module\\Admin\\Composer::assetsInstall"
    },
    "require-dev": {
        "vimeo/psalm": "^4.21"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    }
}
